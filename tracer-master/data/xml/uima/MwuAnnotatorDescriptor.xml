<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>de.uni_leipzig.asv.medusa.uima.analysis_engine.MwuAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>MwuAnnotatorDescriptor</name>
    <description>A MwuAnnotator uses MEDUSA's mwu detection and gives each found mwu an Annotation of the type MWU. It expects a tokenized-view and uses the mwu-file as configured in MEDUSA's configuration.

This annotator does not have parameters.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <types>
        <typeDescription>
          <name>de.uni_leipzig.asv.medusa.uima.types.MWU</name>
          <description>marks a known MWU</description>
          <supertypeName>uima.tcas.Annotation</supertypeName>
        </typeDescription>
      </types>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">de.uni_leipzig.asv.medusa.uima.types.MWU</type>
        </outputs>
        <inputSofas>
          <sofaName>tokenized-sofa</sofaName>
        </inputSofas>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
