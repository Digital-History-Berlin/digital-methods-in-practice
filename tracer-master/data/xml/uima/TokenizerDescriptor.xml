<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>de.uni_leipzig.asv.medusa.uima.analysis_engine.TokenizerAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>TokenizerDescriptor</name>
    <description>A TokenizerAnnotator tokenizes the text of the default view and stores the result in a generated tokenized view. For tokenizing MEDUSA's tokenizer is used as configured in MEDUSA's configuration.

This annotator does not have parameters.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import location="file:SentenceInformationTypeDescriptor.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <outputSofas>
          <sofaName>tokenized-sofa</sofaName>
        </outputSofas>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
